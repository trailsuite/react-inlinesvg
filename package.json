{
  "name": "react-inlinesvg-with-cache",
  "version": "0.8.0",
  "description": "An SVG loader for React, with caching",
  "main": "./lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test-cov": "babel-node isparta cover --report html --report text node_modules/.bn/_mocha",
    "build": "mkdir -p ./lib && NODE_ENV=production babel src --out-file lib/index.js && NODE_ENV=production webpack",
    "clean": "rm -rf lib",
    "lint": "eslint src test",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "httpplease": "0.16.4",
    "once": "1.3.3",
    "react-dom": "0.14.7"
  },
  "devDependencies": {
    "atob": "2.0.0",
    "babel-cli": "6.6.5",
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.5",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-transform-es2015-modules-umd": "6.6.5",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "chai": "^3.4.1",
    "eslint": "2.3.0",
    "eslint-plugin-react": "^3.10.0",
    "httpplease": "0.16.4",
    "isparta": "^4.0.0",
    "jsdom": "8.1.0",
    "mocha": "^2.3.4",
    "mocha-jsdom": "1.1.0",
    "mock-require": "1.2.1",
    "once": "1.3.3",
    "react-addons-test-utils": "0.14.7",
    "react-dom": "0.14.7",
    "testdom": "2.0.0",
    "webpack": "1.12.14"
  },
  "peerDependencies": {
    "react": "*"
  },
  "repository": {
    "type": "git",
