{
  "name": "react-inlinesvg",
  "version": "0.4.2",
  "description": "An SVG loader for React",
  "main": "./lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test-cov": "babel-node isparta cover --report html --report text node_modules/.bin/_mocha",
    "build": "babel src --out-file lib/index.js && babel src --out-file standalone/react-inlinesvg.js",
    "clean": "rm -rf lib",
    "lint": "eslint src test"
  },
  "dependencies": {
    "httpplease": "0.16.4",
    "once": "1.3.3",
    "react-dom": "0.14.7"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "react-dom": "0.14.7",
    "react-addons-test-utils": "0.14.7",
    "atob": "2.0.0",
    "mock-require": "1.2.1",
    "testdom": "2.0.0",
    "jsdom": "8.1.0",
    "mocha-jsdom": "1.1.0",
    "httpplease": "0.16.4",
    "once": "1.3.3",
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.5",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "chai": "^3.4.1",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-react": "^3.10.0",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4"
  },
  "peerDependencies": {
    "react": "*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/matthewwithanm/react-inlinesvg.git"
  },
  "keywords": [
    "react-component",
    "react",
    "svg",
    "component"
  ],
  "author": "Matthew Dapena-Tretter <m@tthewwithanm.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matthewwithanm/react-inlinesvg/issues"
  },
  "browserify-shim": {
    "react": "global:React"
  }
}
